<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="AccentName">Amber</system:String>
    <system:String x:Key="AccentGroup">Windows Phone</system:String>
    <system:Int16 x:Key="SortOrder">140</system:Int16>
    <Color x:Key="AccentColor">#FFF0A30A</Color>
    <SolidColorBrush x:Key="Accent" Color="{DynamicResource AccentColor}" />
</ResourceDictionary>