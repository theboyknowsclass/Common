<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="AccentName">Violet</system:String>
    <system:String x:Key="AccentGroup">Windows Phone</system:String>
    <system:Int16 x:Key="SortOrder">80</system:Int16>
    <Color x:Key="AccentColor">#FFAA00FF</Color>
    <SolidColorBrush x:Key="Accent" Color="{DynamicResource AccentColor}" />
</ResourceDictionary>